<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.akashraj.common_place" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TwitterLoginActivity"
            android:label="@string/title_activity_twitter_login_acitvity" >
        </activity>
        <activity
            android:name=".FacebookLoginActivity"
            android:label="@string/title_activity_facebook_login_acitvity" >
        </activity>
        <activity
            android:name=".GoogleLoginActivity"
            android:label="@string/title_activity_google_login_acitvity" >
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name=".CreateEvent"
            android:label="@string/title_activity_create_event" >
        </activity>
    </application>

</manifest>
